    "mongoose": "^5.11.17",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/redis": "^2.8.28",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5"
